{
    "indent": 4,
    "node": true,
    "maxdepth": 2,
    "nonew": true,
    "noarg": true,
    "immed": true,
    "undef": true,
    "maxparams": 3,
    "forin": false,
    "curly": false,
    "strict": true,
    "eqeqeq": true,
    "esnext": true,
    "newcap": true,
    "unused": true,
    "regexp": true,
    "noempty": true,
    "bitwise": true,
    "latedef": true,
    "trailing": true,
    "plusplus": false,
    "smarttabs": true,
    "camelcase": false,
    "maxstatements": 20,
    "quotmark": "single",
    "globals": {
        "$": true,
        "_": true,
        "dojo": false,
        "jquery": true,
        "module": true,
        "exports": true,
        "require": true,
        "console": false,
        "brackets": true,
        "document": true,
        "mootools": false
    }
}
